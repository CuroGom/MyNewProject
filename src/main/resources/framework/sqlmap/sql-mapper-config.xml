    </settings>

    <typeAliases>
        <typeAlias alias="loginVO" type="com.foo.myapp.login.service.LoginVO"/>
		<typeAlias alias="boardVO" type="com.foo.myapp.board.service.BoardVO"/>
		<typeAlias alias="fileVO" type="com.foo.myapp.common.file.service.FileVO"/>
		<typeAlias alias="atchFileVO" type="com.foo.myapp.common.file.service.AtchFileVO"/>
        <typeAlias alias="searchBoardVO" type="com.foo.myapp.board.service.SearchBoardVO"/>

    </typeAliases>


</configuration>